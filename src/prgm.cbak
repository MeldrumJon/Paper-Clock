#include <Arduino.h>
#include <avr/power.h>
#include <avr/sleep.h>
#include <util/delay.h>
#include <stdio.h>
#include "usart.h"

void setup(void) {
 	power_usart0_enable();
 	usart_init();
 	stdout = &uart_stream;
 	printf("Hello world!\r\n");
}

void loop(void) {	
    _delay_ms(1000);
    printf("test\r\n");
}
